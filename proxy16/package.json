{
    "name": "pocketnetproxy",
    "version": "1.0.0",
    "description": "Pocketnet Proxy Server 16",
    "main": "index.js",
    "scripts": {
        "start": "node cli.js --cli",
        "serve": "set NODE_ENV=production && node --nouse-idle-notification --expose-gc --max-old-space-size=8192 cli.js --cli",
        "serve:proxy": "set NODE_ENV=production && node --nouse-idle-notification --expose-gc --max-old-space-size=8192 cli.js --cli --reverseproxy",
        "servecl": "node cli.js --cli",
        "serveinspect": "node --inspect --nouse-idle-notification --expose-gc --max-old-space-size=8192 cli.js --cli --test",
        "serve:test": "node --nouse-idle-notification --expose-gc --max-old-space-size=8192 cli.js --cli --test",
        "serveTest": "nodemon cli.js --cli --test",
        "serveProd": "nodemon cli.js --cli",
        "servePreProd": "nodemon cli.js --cli --experimentalnodes",
        "install-windows-service": "winser -i",
        "uninstall-windows-service": "winser -r",
        "build": "grunt"
    },
    "author": "Max Grishkov",
    "license": "ISC",
    "dependencies": {
        "@seald-io/nedb": "^4.0.4",
        "arch": "^2.2.0",
        "axios": "^1.6.5",
        "clone-deep": "^4.0.1",
        "compression": "^1.7.4",
        "deep-extend": "^0.6.0",
        "download-github-release": "^0.3.2",
        "express": "^4.21.2",
        "express-useragent": "^1.0.15",
        "firebase-admin": "^12.0.0",
        "hex-captcha": "^2.0.5",
        "is-localhost-ip": "^2.0.0",
        "lambda-rate-limiter": "3.0.1",
        "lodash": "^4.17.15",
        "md5": "^2.3.0",
        "node-fetch": "^2.7.0",
        "nodejs-base64": "^1.0.3",
        "random": "^2.2.0",
        "redaxios": "^0.5.1",
        "request": "^2.88.2",
        "request-progress": "^3.0.0",
        "socks-proxy-agent": "^7.0.0",
        "svg-captcha": "^1.4.0",
        "targz": "^1.0.1",
        "tcp-port-used": "^1.0.1",
        "tree-kill": "^1.2.2",
        "underscore": "",
        "winston": "^3.3.3",
        "ws": "^8.16.0"
    },
    "devDependencies": {
        "@types/grunt": "^0.4.31",
        "grunt": "^1.6.1",
        "grunt-cli": "^1.4.3",
        "nodemon": "^3.0.3",
        "winser": "^1.0.3"
    }
}