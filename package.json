    "build": {
        "productName": "Bastyon",
        "afterSign": "electron-builder-notarize",
        "appId": "app.pocketnet.gui",
        "files": [
            "**/*",
            "!cordova${/*}",
            "!external${/*}",
            "!proxy16/data${/*}",
            "!proxy16/pocketcoin${/*}",
            "!proxy16/pocketcoind${/*}",
            "!proxy16/downloads${/*}"
        ],
        "extraFiles": [
            {
                "from": "proxy16/cert",
                "to": "cert",
                "filter": [
                    "**/cert.pem",
                    "**/key.pem"
                ]
            },
            {
                "from": "proxy16/tor",
                "to": "tor",
                "filter": [
                    "**/*"
                ]
            },
            {
                "from": "proxy16/static",
                "to": "static",
                "filter": [
                    "**/"
                ]
            }
        ],
        "protocols": {
            "name": "bastyon",
            "schemes": [
                "bastyon",
                "pocketnet"
            ]
        },
        "mac": {
            "target": "dmg",
            "artifactName": "${productName}Setup.${ext}",
            "icon": "res/electron/icons/mac/icon.icns",
            "extendInfo": "bastyon",
            "hardenedRuntime": true,
            "entitlements": "./node_modules/electron-builder-notarize/entitlements.mac.inherit.plist"
        },
        "linux": {
            "target": [
                {
                    "target": "AppImage"
                },
                {
                    "target": "deb"
                }
            ],
            "artifactName": "${productName}Setup.${ext}",
            "category": "Network",
            "executableName": "bastyon",
            "description": "Bastyon Desktop Browser"
        },
        "appImage": {
            "artifactName": "${productName}.${ext}"
        },
        "win": {
            "target": [
                {
                    "target": "nsis"
                }
            ],
            "icon": "res/electron/icons/win/icon.ico"
        },
        "nsis": {
            "artifactName": "${productName}Setup.${ext}",
            "oneClick": true,
            "perMachine": false,
            "allowToChangeInstallationDirectory": false,
            "createDesktopShortcut": true,
            "displayLanguageSelector": true,
            "menuCategory": true,
            "installerIcon": "res/electron/icons/win/icon.ico",
            "uninstallerIcon": "res/electron/icons/win/icon.ico",
            "uninstallDisplayName": "${productName}, v${version}",
            "deleteAppDataOnUninstall": false,
            "license": "LICENSE",
            "include": "build/installer.nsh"
        }
    },
    "buildTest": {
        "productName": "BastyonTest",
        "afterSign": "build/notarize.js",
        "appId": "app.pocketnettest.gui",
        "files": [
            "**/*",
            "!cordova${/*}",
            "!external${/*}",
            "!proxy16/data${/*}",
            "!proxy16/pocketcoin${/*}",
            "!proxy16/pocketcoind${/*}",
            "!proxy16/downloads${/*}"
        ],
        "extraFiles": [
            {
                "from": "proxy16/cert",
                "to": "cert",
                "filter": [
                    "**/cert.pem",
                    "**/key.pem"
                ]
            },
            {
                "from": "proxy16/static",
                "to": "static",
                "filter": [
                    "**/"
                ]
            }
        ],
        "protocols": {
            "name": "bastyon",
            "schemes": [
                "bastyon",
                "pocketnet"
            ]
        },
        "mac": {
            "target": "dmg",
            "artifactName": "${productName}Setup.${ext}",
            "icon": "res/electron/icons/mac/icon.icns",
            "entitlements": "build/entitlements.mac.plist",
            "extendInfo": "bastyon"
        },
        "linux": {
            "target": [
                {
                    "target": "AppImage"
                },
                {
                    "target": "deb"
                }
            ],
            "artifactName": "${productName}_${platform}_${arch}.${ext}",
            "category": "Network",
            "description": "${productName} Desktop Browser"
        },
        "win": {
            "target": [
                {
                    "target": "nsis"
                }
            ],
            "icon": "res/electron/icons/win/test_icon.ico"
        },
        "nsis": {
            "artifactName": "${productName}Setup.${ext}",
            "oneClick": true,
            "perMachine": false,
            "allowToChangeInstallationDirectory": false,
            "createDesktopShortcut": true,
            "displayLanguageSelector": true,
            "menuCategory": true,
            "installerIcon": "res/electron/icons/win/test_icon.ico",
            "uninstallerIcon": "res/electron/icons/win/test_icon.ico",
            "uninstallDisplayName": "${productName}, v${version}",
            "deleteAppDataOnUninstall": false,
            "license": "LICENSE",
            "include": "build/installerTest.nsh"
        }
    }
